						<element name="metastase" type="ans:dm_metastase"/>
						<element name="tipoQuimioterapia" type="ans:dm_tipoQuimioterapia"/>
						<element name="planoTerapeutico" type="ans:st_texto1000"/>
					</sequence>
				</complexType>
			</element>
			<element name="drogasSolicitadas">
				<complexType>
