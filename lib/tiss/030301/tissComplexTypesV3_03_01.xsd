												<element name="justificativaItem" type="ans:st_texto150"/>
											</sequence>
										</complexType>
									</element>
								</choice>
							</complexType>
						</element>
					</sequence>
				</complexType>
			</element>
			<element name="observacao" type="ans:st_texto500" minOccurs="0"/>
			<element name="valorTotalRecursado" type="ans:st_decimal10-2"/>
		</sequence>
